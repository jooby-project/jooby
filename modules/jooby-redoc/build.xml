<?xml version="1.0"?>
<project default="redoc">

  <target name="redoc">
    <property name="output" value="${basedir}${file.separator}target${file.separator}classes${file.separator}redoc" />
    <mkdir dir="${output}" />

    <get quiet="true" usetimestamp="true" src="https://cdn.redoc.ly/redoc/latest/bundles/redoc.standalone.js" dest="${output}${file.separator}redoc.standalone.js"/>
  </target>

</project>
